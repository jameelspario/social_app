<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDD766"
    android:animateLayoutChanges="false"
    android:id="@+id/parent_frame_layout"
    tools:context=".deificpk.frags.VerticalParentFragment">

    <ImageView
        android:id="@+id/image_profile_top_layout_vertical_parent_frag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"/>

    <TextView
        android:id="@+id/txt_top_layout_vertical_parent_frag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textSize="24sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:text="no live"/>

    <FrameLayout
        android:id="@+id/top_frame_container_vertical_parent_frag"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


</FrameLayout>